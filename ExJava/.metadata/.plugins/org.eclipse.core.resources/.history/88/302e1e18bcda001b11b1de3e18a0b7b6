
public class ExemploExercicios {
	
	public static void main(String[] args) {
		
		int idade = 19;
		int quantidadePessoas = 3;
		
		if(idade >= 18) {
			System.out.println("Você pode entrar na festa!");
		}  else if(idade <18 && quantidadePessoas >=2)
			System.out.println("Você não é de maior, mas como está acompanhado de alguém poderá entrar");
		}
		
		
		
		
		
		
		
	}

		
		
		
		
	


