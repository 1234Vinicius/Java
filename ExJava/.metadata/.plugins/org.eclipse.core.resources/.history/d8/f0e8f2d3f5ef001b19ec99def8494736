
public class Alis {
	
	public static void main(String[] args) {
		
		
	int idade = 18;
	
	
	
	if(idade >= 18 && idade <=60) {
		System.out.println("ADULTO");
	}
	
	else if(idade <=17 && idade >=13) {
		System.out.println("ADOLESCENTE");
	}
	
	else if(idade <=12 ) {
		System.out.println("CRIANÇA");
	
	
	
	}
}
	