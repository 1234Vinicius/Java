import java.util.Scanner;

public class Cor {
	
	public static void main(String[] args) {
		
		
		
		String cor;
		String corPreferida = "Azul";
		
		System.out.println("Qual a sua cor preferida");
		cor = new Scanner(System.in).nextLine();
		
		System.out.println(corPreferida + " É uma cor bonita");
		
		
		
	}

}
