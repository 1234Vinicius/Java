
public class ExemploExercicios {
	
	public static void main(String[] args) {
		
		
		int salario = 10000;
		
		if(salario <=1085) {
			System.out.println("Você recebe um salário minímo");
		}
		if (salario ==5000) {
			System.out.println("Você recebe 5 salários minímos");
		}
		if(salario == 10000) {
			System.out.println("Você recebe 10 salários minímos");
		}
		
		
	}
}
		
		
		
		
	


