
public class Media {
	
	public static void main(String[] args) {
		
		
		int idade = 18;
		
		if(idade  >=18 && idade <=60) {
			System.out.println("Adulto");
		} else if(idade <=17 && idade >=12) {
			System.out.println("Adolescente");
		} else if (idade <=11 ) {
			System.out.println("Criança");
		} else 
			System.out.println("idoso");
		
	
	
		
	}
		

}


