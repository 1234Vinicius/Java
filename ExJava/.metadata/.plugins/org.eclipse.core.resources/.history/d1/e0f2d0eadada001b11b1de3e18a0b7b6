import java.util.Scanner;

public class Horario {
	
	public static void main(String[] args) {
		
		
		String nome = "Maria";
		
		
		if(nome == "Vinicius" ) {
			System.out.println("Que nome lindo você tem " + nome);
		
		if(nome == "Maria") {
			System.out.println("Que nome lindo você tem " + nome);
			
		}
		} else {
			System.out.println("Que nome comum você tem");
		}
		
		
		



		
		}
	}


