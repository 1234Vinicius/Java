
public class TestaWhile {
	
	public static void main(String[] args) {
		
		int cores = 11;
		
		switch(cores) {
		
		case 1:
			System.out.println("Você escolheu a cor amarela");
			break;
			
		case 2:
			System.out.println("Você escolhe a cor verde");
			break;
			
			
			default:
				System.out.println("Cor inválida");
		
		
		}
	}

}
