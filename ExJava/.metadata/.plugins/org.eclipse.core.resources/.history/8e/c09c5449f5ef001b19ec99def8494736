
public class Alis {
	
	public static void main(String[] args) {
		
		
	int idade = 29;
	
	if(idade >18) {
		System.out.println("ADULTO!");
		 if(idade <=17 && idade >=13) {
			System.out.println("ADOLESCENTE");
		if(idade <=12) {
			System.out.println("CRIANÇA");
		} else {
			System.out.println("IDOSO");
		}
		 }
	
	

		}
	}

}
