
public class TesteVariaveis {

	public static void main(String[] args) {
		System.out.println("testando");
		
		
		int idade;
		idade = 19;
		
		System.out.println("a idade é "idade);
		
		idade = 30 + 10;
		
		System.out.println("a idade é "idade);
		
		idade = (7*5) + 2;
		
		System.out.println("a idade é "idade);
		
		System.out.println("a idade é " + idade);
		
	}
}
