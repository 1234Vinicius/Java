import java.util.Scanner;

public class Fatorial {
	
	public static void main(String[] args) {
		
				
			for(int linha = 0; linha <10; linha++) {
				for(int coluna = 0; linha<=10; coluna++) {
					System.out.print("*");
				}
				System.out.println();
		       }
		    }
		
		
		
	            
	

		  }
	